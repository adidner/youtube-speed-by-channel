{
    "manifest_version": 3,
    "name": "Youtube Speed by Channel",
    "description": "Sets the speed of youtube videos based on Channel",
    "version": "1.0.0",
    "background": {
        "service_worker": "background.js"
    },
    "permissions": ["storage", "activeTab", "scripting", "webNavigation", "history"],
    "host_permissions": [
        "https://youtube.com/*",
        "https://*/*"
    ],
    "action": {
        "default_popup": "popup/popup.html"
    }
}